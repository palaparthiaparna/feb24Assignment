def function(x):
    A=[]

    for n in x:
        if n <= 1:
            continue
        for i in range(2, n):
            if (n % i) == 0:
                break
        else:
            print(n)
            A.append(n)

    return len(A)

ln=[]
z = [1,2,3,4,5]
print('count of prime numbers is: ',function(z))
